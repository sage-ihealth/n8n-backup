    {
      "parameters": {
        "jsCode": "const items = $node[\"Get Subfolders4\"].json.files || [];\nconst currentQueue = $node[\"Pop Folder2\"].json.queue || [];\nconst currentAllPdfs = $node[\"Pop Folder2\"].json.allPdfs || [];\n\nconst newFolders = items.filter(f => f.mimeType === 'application/vnd.google-apps.folder').map(f => f.id);\nconst newPdfs = items.filter(f => f.mimeType === 'application/pdf');\nreturn {\n  queue: [...currentQueue, ...newFolders],\n  allPdfs: [...currentAllPdfs, ...newPdfs]\n};"
