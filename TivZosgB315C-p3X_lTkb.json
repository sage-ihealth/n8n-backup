    {
      "parameters": {
        "jsCode": "const items = $input.all();\nconst allFiles = items[0].json.files || [];\n\nconst folders = allFiles.filter(f => f.mimeType === 'application/vnd.google-apps.folder');\nconst rootPdfs = allFiles.filter(f => f.mimeType === 'application/pdf');\n\n// 保存根目录的 PDF 到工作流数据\n$('Workflow').first().json.rootPdfs = rootPdfs;\n\nreturn folders.map(folder => ({\n  json: {\n    folderId: folder.id,\n    folderName: folder.name\n  }\n}));"
      },
